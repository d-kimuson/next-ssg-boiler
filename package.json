{
  "name": "next-ssg-boiler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "PORT=3000 next dev",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build && next export",
    "analyze": "ANALYZE=true yarn build",
    "start": "yarn build && http-server out -p 80",
    "typecheck": "tsc -p . --noEmit",
    "lint": "yarn typecheck && run-p lint:*",
    "lint:eslint": "eslint src --ext .ts,.tsx,.jsx",
    "lint:stylelint": "stylelint 'src/styles/**/*.{css,scss}'",
    "lint:prettier": "prettier '**/*.{ts,tsx,js,jsx,json,css,scss}' --check",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "fix:prettier": "prettier '**/*.{ts,tsx,js,jsx,json,css,scss}' --write",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.3.0",
    "aspida": "^1.7.1",
    "axios": "^0.21.1",
    "immer": "^9.0.5",
    "modern-css-reset": "^1.4.0",
    "next": "^11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "^0.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "add": "^2.0.6",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "http-server": "^0.12.3",
    "husky": "4",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.13.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.2",
    "tsc": "^1.20150623.0",
    "typescript": "4.1",
    "yarn": "^1.22.10"
  }
}
